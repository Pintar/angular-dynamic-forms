{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "simple-dynamic-forms": {
      "root": "projects/simple-dynamic-forms",
      "sourceRoot": "projects/simple-dynamic-forms/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/simple-dynamic-forms/tsconfig.lib.json",
            "project": "projects/simple-dynamic-forms/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/simple-dynamic-forms/src/test.ts",
            "tsConfig": "projects/simple-dynamic-forms/tsconfig.spec.json",
            "karmaConfig": "projects/simple-dynamic-forms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/simple-dynamic-forms/tsconfig.lib.json",
              "projects/simple-dynamic-forms/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "forms-showcase": {
      "root": "projects/forms-showcase/",
      "sourceRoot": "projects/forms-showcase/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/forms-showcase",
            "index": "projects/forms-showcase/src/index.html",
            "main": "projects/forms-showcase/src/main.ts",
            "polyfills": "projects/forms-showcase/src/polyfills.ts",
            "tsConfig": "projects/forms-showcase/tsconfig.app.json",
            "assets": [
              "projects/forms-showcase/src/favicon.ico",
              "projects/forms-showcase/src/assets"
            ],
            "styles": [
              "projects/forms-showcase/src/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/forms-showcase/src/environments/environment.ts",
                  "with": "projects/forms-showcase/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "forms-showcase:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "forms-showcase:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "forms-showcase:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/forms-showcase/src/test.ts",
            "polyfills": "projects/forms-showcase/src/polyfills.ts",
            "tsConfig": "projects/forms-showcase/tsconfig.spec.json",
            "karmaConfig": "projects/forms-showcase/karma.conf.js",
            "styles": [
              "projects/forms-showcase/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/forms-showcase/src/favicon.ico",
              "projects/forms-showcase/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/forms-showcase/tsconfig.app.json",
              "projects/forms-showcase/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "forms-showcase-e2e": {
      "root": "projects/forms-showcase-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/forms-showcase-e2e/protractor.conf.js",
            "devServerTarget": "forms-showcase:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "forms-showcase:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/forms-showcase-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "bootstrap-components": {
      "projectType": "library",
      "root": "projects/bootstrap-components",
      "sourceRoot": "projects/bootstrap-components/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/bootstrap-components/tsconfig.lib.json",
            "project": "projects/bootstrap-components/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/bootstrap-components/src/test.ts",
            "tsConfig": "projects/bootstrap-components/tsconfig.spec.json",
            "karmaConfig": "projects/bootstrap-components/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/bootstrap-components/tsconfig.lib.json",
              "projects/bootstrap-components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "simple-dynamic-forms"
}